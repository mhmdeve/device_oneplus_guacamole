    <!-- SRGB display mode sysfs node path -->
    <string name="node_srgb_mode_switch">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/native_display_srgb_color_mode</string>

    <!-- Customer SRGB display mode sysfs node path -->
